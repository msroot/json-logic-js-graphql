{
  "name": "json-logic-js-graphql",
  "version": "1.5",
  "description": "Graphql query support for json-logic-js using Lodash methods",
  "main": "./lib/index.js",
  "scripts": {
    "test": "pnpm build && jest",
    "build": "babel ./src/ --out-dir ./lib/",
    "watch": "babel -w ./src/ --out-dir ./lib/",
    "prettier": "prettier --config ./.prettierrc.js --write \"**/*.js\"",
    "lint": "pnpm prettier --write --check --config ./.prettierrc.js \"**/*.js\" && eslint --fix ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msroot/json-logic-js-graphql.git"
  },
  "keywords": [
    "json",
    "logic",
    "jsonlogic",
    "rules",
    "graphql",
    "lodash"
  ],
  "author": "msroot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msroot/json-logic-js-graphql/issues"
  },
  "homepage": "https://github.com/msroot/json-logic-js-graphql#readme",
  "dependencies": {
    "date-fns": "2.29.3",
    "json-logic-js": "2.0.2",
    "lodash": "4.17.21",
    "set-value": "4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@types/jest": "29.2.3",
    "@types/json-logic-js": "2.0.1",
    "@types/lodash": "4.14.191",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.3.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "glob-parent": "6.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "tsc-files": "1.1.3",
    "tslib": "2.4.1",
    "typescript": "4.9.3"
  }
}
